{
  "name": "tabtab",
  "displayName": "guzzhao",
  "version": "0.0.1",
  "private": true,
  "description": "tab tab tools",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "node scripts/prepare.js",
    "dev:web": "vite",
    "dev:background": "vite build --config vite.config.background.js --mode development",
    "dev:content": "vite build --config vite.config.content.js --mode development",
    "build": "cross-env NODE_ENV=production run-s  build:web build:prepare build:background build:js",
    "build:prepare": "node scripts/prepare.js",
    "build:web": "vite build",
    "build:background": "vite build --config vite.config.background.js",
    "build:content": "vite build --config vite.config.content.js",
    "pack": "cross-env NODE_ENV=production run-p pack:*"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "crypto-js": "^4.2.0",
    "mathjs": "^14.3.0",
    "nanoid": "^5.1.2",
    "npm-run-all": "^4.1.5",
    "pinia": "^3.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.313",
    "@tailwindcss/vite": "4.0.9",
    "@types/fs-extra": "^11.0.4",
    "@types/webextension-polyfill": "^0.12.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/core": "^12.7.0",
    "chokidar": "^4.0.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "daisyui": "5.0.0",
    "fs-extra": "^11.3.0",
    "kolorist": "^1.8.0",
    "tailwindcss": "4.0.9",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.0",
    "vue": "^3.5.13",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.12.0"
  },
  "packageManager": "pnpm@10.5.2"
}
